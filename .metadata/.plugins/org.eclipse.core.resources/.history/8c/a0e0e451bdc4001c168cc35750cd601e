package com.javanaveia.bancodedadosmysql.bancodedadosmysql;

import java.sql.Connection;

import com.javanaveia.bancodedadosmysql.bancodedadosmysql.dao.AlunoDAO;
import com.javanaveia.bancodedadosmysql.bancodedadosmysql.model.Aluno;

/**
 * Hello world!
 *
 */
public class App {
	public static void main(String[] args) throws ClassNotFoundException {

		Connection conn = null;

		AlunoDAO dao = new AlunoDAO();
		System.out.println(dao.getAluno().toString());
		System.out.println("---------------------------------------");
		
		System.out.println(dao.findById(10));
		
		System.out.println("---------------------------------------");
		
		Aluno al = new Aluno();
		al.setNome("Tania");
		al.setIdade(46);
		al.setEstado("PE");
		dao.create(al);
		dao.getAluno().forEach(System.out::println);
	}
}
