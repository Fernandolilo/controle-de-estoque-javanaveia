/**
 * Hello world!
 *
 */
public class App {
	public static void main(String[] args) {

		Connection conn = null;
		
		try {
			conn = ConnectionFactory.getConnection();
			if(conn != null) {
				System.out.println("Conex√£o com banco de dados test com Sucesso!!!");
			}
		} catch (ClassNotFoundException e) {
			System.out.println("Falha.........Falha.......Falha");
			e.printStackTrace();
		}
	}
}
